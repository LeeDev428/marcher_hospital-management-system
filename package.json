{
	"name": "@marcher/his",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "nuxt build",
		"dev": "dotenvx run -- nuxt dev --host",
		"prod": "dotenvx run -- node .output/server/index.mjs",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"format": "prettier --write .",
		"lint": "eslint .",
		"prisma:generate:instance": "prisma generate --schema=./prisma/instance",
		"prisma:push:instance": "prisma db push --schema=./prisma/instance",
		"prisma:migrate:instance": "prisma migrate dev --schema=./prisma/instance",
		"prisma:reset:instance": "prisma migrate reset --schema=./prisma/instance --force",
		"prisma:studio:instance": "prisma studio --schema=./prisma/instance",
		"prisma:generate:global": "prisma generate --schema=./prisma/global",
		"prisma:push:global": "prisma db push --schema=./prisma/global",
		"prisma:migrate:global": "prisma migrate dev --schema=./prisma/global",
		"prisma:reset:global": "prisma migrate reset --schema=./prisma/global --force",
		"prisma:studio:global": "prisma studio --schema=./prisma/global",
		"db:setup": "npm run prisma:generate:global && npm run prisma:generate:instance && npm run prisma:push:global && npm run prisma:push:instance",
		"db:reset": "npm run prisma:reset:global && npm run prisma:reset:instance && npm run db:setup"
	},
	"dependencies": {
		"@dotenvx/dotenvx": "^1.44.2",
		"@nuxt/eslint": "^1.4.1",
		"@nuxt/fonts": "^0.11.4",
		"@nuxt/icon": "^1.13.0",
		"@nuxt/image": "^1.10.0",
		"@pinia/nuxt": "^0.11.1",
		"@prisma/client": "^6.13.0",
		"@radix-ui/react-select": "^2.2.5",
		"@tanstack/vue-table": "^8.21.3",
		"@trpc/client": "^11.4.1",
		"@trpc/server": "^11.4.1",
		"@vee-validate/zod": "^4.15.1",
		"@vueuse/core": "^13.4.0",
		"@whoicd/icd11ect": "^1.7.1",
		"axios": "^1.12.2",
		"bcrypt": "^6.0.0",
		"chart.js": "^4.5.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"eslint": "^9.29.0",
		"i": "^0.3.7",
		"jsonwebtoken": "^9.0.2",
		"lru-cache": "^11.1.0",
		"lucide-vue-next": "^0.516.0",
		"minio": "^8.0.5",
		"nodemailer": "^7.0.5",
		"npm": "^11.4.2",
		"nuxt": "^3.17.5",
		"pinia": "^3.0.3",
		"pinia-plugin-persistedstate": "^4.3.0",
		"qr-scanner": "^1.4.2",
		"qrcode": "^1.5.4",
		"reka-ui": "^2.3.1",
		"shadcn-nuxt": "^2.2.0",
		"tailwind-merge": "^3.3.1",
		"tesseract.js": "^6.0.1",
		"trpc-nuxt": "^1.0.5",
		"tw-animate-css": "^1.3.4",
		"vee-validate": "^4.15.1",
		"vue": "^3.5.16",
		"vue-chartjs": "^5.3.2",
		"vue-router": "^4.5.1",
		"vue-sonner": "^2.0.0",
		"zod": "^3.25.67"
	},
	"devDependencies": {
		"@iconify-json/lucide": "^1.2.68",
		"@iconify-json/mdi": "^1.2.3",
		"@tailwindcss/vite": "^4.1.10",
		"@types/bcrypt": "^5.0.2",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/nodemailer": "^6.4.17",
		"@types/qrcode": "^1.5.5",
		"@vue/devtools-api": "^7.7.7",
		"nitropack": "^2.11.13",
		"prettier": "^3.5.3",
		"prisma": "^6.13.0",
		"tailwindcss": "^4.1.10",
		"typescript": "^5.9.3"
	}
}
