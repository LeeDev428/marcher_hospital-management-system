enum StaffRole {
    ADMIN
    DOCTOR
    NURSE
    SECRETARY
    STAFF
}

model StaffProfile {
    id         String   @id @default(uuid()) @db.Uuid
    lastName   String
    firstName  String
    middleName String?
    suffix     String?
    role       StaffRole
    profession String?
    createdAt  DateTime @default(now())
    updatedAt  DateTime @updatedAt

    @@index([id])
}
